<div id="content-header">
      <img src="/images/show_finance.png" alt="" />
  <h1>Fee collection dates</h1>
  <h3>View fee collection dates</h3>
  <div id="app-back-button">
    <%= link_to_function image_tag("/images/buttons/back.png",:border => 0), "history.back()" %>
  </div>
</div>
<div id="page-yield">

  <div id="batch">
    <div class="label-field-pair"><label>Select a batch : </label>
      <div class="text-input-bg"> <%= select("id",:id,@batchs.map { |c| [c.full_name, c.id] },
        {:prompt => "Select a batch"},
        {:onChange => "#{remote_function(:url => {:action => "fee_collection_dates_batch"}, :with => "'id='+value")}"})%> </div>
  </div>
  </div>
  <div id="fee_collection_dates"></div>
  <div id="modal-box" style="display:none;"></div>

</div>